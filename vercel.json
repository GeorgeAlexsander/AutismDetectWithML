{
    "version": 2,
    "builds": [
      {
        "src": "src/frontend/package.json",
        "use": "@vercel/static-build",
        "config": { "distDir": "build" }
      },
      {
        "src": "src/backend/AutismPredictionAPI.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb" }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "src/backend/AutismPredictionAPI.py"
      },
      {
        "src": "/(.*)",
        "dest": "src/frontend/build/$1"
      }
    ]
  }
  