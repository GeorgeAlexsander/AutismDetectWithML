{
    "version": 2,
    "builds": [
      {
        "src": "src/frontend/package.json",
        "use": "@vercel/static-build"
      },
      {
        "src": "src/backend/AutismPredictionAPI.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/src/frontend/$1"
      },
      {
        "src": "/api/(.*)",
        "dest": "/src/backend/$1"
      }
    ]
  }
  